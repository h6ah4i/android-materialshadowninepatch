    <style name="ms9_DefaultShadowStyleBase">
        <item name="ms9_ambientShadowDrawablesList">@null</item>
        <item name="ms9_spotShadowDrawablesList">@null</item>
        <item name="ms9_forceUseCompatShadow">false</item>
